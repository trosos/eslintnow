{
  "name": "eslintnow",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@babel/plugin-transform-runtime": "latest",
    "@babel/preset-env": "latest",
    "@babel/runtime-corejs3": "latest",
    "@types/node": "latest",
    "babel-loader": "latest",
    "copyfiles": "latest",
    "diff": "latest",
    "eslint": "latest",
    "fs-teardown": "latest",
    "jsbi": "^4.3.0",
    "lodash.merge": "latest",
    "memfs": "latest",
    "node-polyfill-webpack-plugin": "latest",
    "npm-run-all": "latest",
    "regexpu-core": "latest",
    "rimraf": "latest",
    "ts-loader": "latest",
    "typedarray": "latest",
    "typescript": "latest",
    "webpack-cli": "latest"
  },
  "scripts": {
    "clean": "node ./src/npm-helper.js clean",
    "distclean": "node ./src/npm-helper.js distclean",
    "wrap": "node ./src/npm-helper.js wrap",
    "install": "npm-run-all -s install:*",
    "install:git": "npm-run-all -s install:git:*",
    "install:git:jsbi": "npm-run-all -s install:git:jsbi:*",
    "install:git:jsbi:rmdst": "rimraf ./jsbi-git",
    "install:git:jsbi:clone": "git clone https://github.com/GoogleChromeLabs/jsbi.git jsbi-git",
    "install:git:jsbi:checkout": "git -C jsbi-git checkout c40371e089d1433bd8ff20b7a5be617d0561e85c",
    "install:git:tests": "npm-run-all -s install:git:tests:*",
    "install:git:tests:rmdst": "rimraf ./eslint-git",
    "install:git:tests:clone": "git clone https://github.com/eslint/eslint.git eslint-git",
    "install:git:tests:checkout": "git -C eslint-git checkout d8c8ede088e1f4a82c9b6b5c2772af268b9161aa",
    "build": "npm-run-all -s build:*",
    "build:webpack": "webpack",
    "build:ruleutils": "copyup ./src/ruleUtils.js ./dist",
    "build:ruletester": "copyup ./src/ruleTester.js ./dist",
    "build:tests": "npm-run-all -s build:tests:*",
    "build:tests:chunks": "node ./src/extract-tests.js",
    "build:tests:glue": "copyup ./src/ruleTests.js ./dist",
    "build:filltemplates": "node ./src/fill-templates.js"
  }
}
